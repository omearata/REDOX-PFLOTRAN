  MICROBIAL_REACTION 
    REACTION CH4(aq) + SO4-- + H+ -> HCO3- + H2O + H2S(aq) 
    !RATE_CONSTANT 9.5d-16 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec)
    RATE_CONSTANT 2.3d-15 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec)
    MONOD
      SPECIES_NAME SO4--
      HALF_SATURATION_CONSTANT 1.3d-6  #Gao et al 2010
      !HALF_SATURATION_CONSTANT 1.3d-10  #Gao et al 2010
    /
    MONOD
      SPECIES_NAME CH4(aq)
      HALF_SATURATION_CONSTANT 1.3d-6  #Gao et al 2010
    /
    BIOMASS
      SPECIES_NAME microbes
      YIELD 0.1d0
    /
  /


#Methanogenesis HCO3-
  MICROBIAL_REACTION 
    REACTION HCO3- + 7.d0 H+ -> 1.d0 CH4(aq) + 2.d0 H2O
    !RATE_CONSTANT 4.33d-13 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec) 3.1d-4 -14
    RATE_CONSTANT 9.33d-16 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec) 3.1d-4 -14
    MONOD
      SPECIES_NAME HCO3-
      !HALF_SATURATION_CONSTANT 8.d-7  #Gao et al 2010
      HALF_SATURATION_CONSTANT 8.d-5  #Gao et al 2010
    /
    BIOMASS
      SPECIES_NAME microbes
      YIELD 0.1d0
    /
  /


#Methanogenesis DOM1
  MICROBIAL_REACTION 
    REACTION DOM1 + H2O -> 1.d0 CH4(aq) + 1.d0 HCO3- + 1.d0 H+ + 3.d-1 NH4+ 
    !RATE_CONSTANT 9.83d-14 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec) 0.8d-3 -15
    RATE_CONSTANT 8.83d-16 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec) 0.8d-3 -15
    MONOD
      SPECIES_NAME DOM1
      HALF_SATURATION_CONSTANT 9.d-5  #Gao et al 2010
    /
    BIOMASS
      SPECIES_NAME microbes
      YIELD 0.1d0
    /
  /

#Methanogenesis DOM2
  MICROBIAL_REACTION 
    REACTION DOM2 + 6.d0 H+ + H2O -> 4.d0 CH4(aq) + 2.d0 HCO3- + 3.d-1 NH4+
    RATE_CONSTANT 6.83d-16 #Thompson et al 1995 mol-m3 bulk/(L water-mol biomass-sec) 0.8d-4 -16
    MONOD
      SPECIES_NAME DOM2
      HALF_SATURATION_CONSTANT 1.3d-5  #Gao et al 2010
    /
    BIOMASS
      SPECIES_NAME microbes
      YIELD 0.1d0
    /
  /

  # Ebullition
  MICROBIAL_REACTION
    REACTION 1.00e+00 CH4(aq) + 0.006d0 H2S(aq) + 0.001d0 N2O(aq) + HCO3- -> 1.00e+00 Tracer
    !REACTION 1.00e+00 CH4(aq) -> 1.00e+00 Tracer
    RATE_CONSTANT       3.00e-05
    MONOD
      SPECIES_NAME        CH4(aq)
      HALF_SATURATION_CONSTANT 4.00e-04
    /
    INHIBITION
      SPECIES_NAME        CH4(aq)
      TYPE THRESHOLD 7064445
      INHIBITION_CONSTANT 5.d-4
    /
  /

 # LOG_FORMULATION
  DATABASE ./TAI_database.dat

  OUTPUT
    PH
  skip
    FREE_ION
    !  O2(aq)
      SO4--
      CH4(aq)
    !  DOM1
    !  DOM2
    !  NO3-
      NH4+
    !  HCO3-
    !  Tracer
    !  Tracer2
  noskip

    TOTAL
      SO4--
!      Fe(OH)2(aq)
      Fe(OH)3(aq)
!      HS-
      H2S(aq)
!      HCO3-
       CH4(aq)
       NH4+
!       O2(aq)
!       NO3-

    !PRIMARY_SPECIES
    !SECONDARY_SPECIES
    !IMMOBILE
    !GASES
    !MINERALS
  /
END

